{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/static-build" }],
  "routes": [
    {
      "src": "/favicon.svg",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/favicon.svg"
    },
    {
      "src": "/og.png",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/og.png"
    },
    {
      "src": "/robots.txt",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      },
      "dest": "/robots.txt"
    },
    {
      "src": "/fonts/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/fonts/$1"
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/images/$1"
    },
    {
      "src": "/_astro/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/_astro/$1"
    },
    {
      "src": "/~partytown/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/~partytown/$1"
    },
    {
      "src": "/(.*)",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https: data:; worker-src 'self' blob:; connect-src 'self' https:; img-src 'self' data: https:; font-src 'self' https: data:; style-src 'self' 'unsafe-inline' https:; frame-src 'self' https:; object-src 'none';",
        "X-Frame-Options": "DENY",
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "strict-origin-when-cross-origin",
        "Strict-Transport-Security": "max-age=63072000; includeSubDomains; preload",
        "Permissions-Policy": ""
      },
      "dest": "/$1"
    }
  ]
}
